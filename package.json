    "release": "bumpp -r --no-push",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@discordjs/core": "2.0.1",
    "@nuxtjs/robots": "5.2.8",
    "@vueuse/core": "13.0.0",
    "@vueuse/math": "13.0.0",
    "@vueuse/nuxt": "13.0.0",
    "@vueuse/shared": "13.0.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@fontsource/roboto": "5.2.5",
    "@nuxt/devtools": "2.3.0",
    "@nuxt/test-utils": "3.17.2",
    "@nuxtjs/i18n": "9.3.2",
    "@nuxtjs/tailwindcss": "6.13.2",
    "@rotki/eslint-config": "4.0.1",
    "@rotki/ui-library": "1.12.2",
    "@types/node": "20.17.24",
    "@vitest/coverage-v8": "3.0.9",
    "@vitest/ui": "3.0.9",
    "@vue/test-utils": "2.4.6",
    "bumpp": "10.1.0",
    "eslint": "9.22.0",
    "eslint-plugin-nuxt": "4.0.0",
    "happy-dom": "16.8.1",
    "husky": "9.1.7",
    "lint-staged": "15.5.0",
    "npm-run-all2": "7.0.2",
    "nuxt": "3.16.0",
    "sass": "1.86.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.2",
    "vitest": "3.0.9",
    "vue-tsc": "2.2.8",
    "zlib-sync": "0.1.9"
  },
  "engines": {
    "node": ">=20 <21",
    "pnpm": ">=10 <11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint"
  }
