{
  "name": "@rotki/discord-captcha",
  "version": "0.6.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "type": "module",
  "scripts": {
    "build": "run-p typecheck \"build:site {@}\" --",
    "build:site": "nuxi build",
    "dev": "nuxi dev",
    "generate": "nuxi generate",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
