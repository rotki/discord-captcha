    "prepare": "husky",
    "release": "bumpp -r --no-push",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@discordjs/core": "2.3.0",
    "@nuxtjs/robots": "5.5.6",
    "@vueuse/core": "14.0.0",
    "@vueuse/math": "14.0.0",
    "@vueuse/nuxt": "14.0.0",
    "@vueuse/shared": "14.0.0",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@fontsource/roboto": "5.2.8",
    "@nuxt/devtools": "3.0.1",
    "@nuxt/test-utils": "3.20.1",
    "@nuxtjs/i18n": "10.2.0",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@rotki/eslint-config": "4.5.0",
    "@rotki/eslint-plugin": "^1.2.0",
    "@rotki/ui-library": "2.7.0",
    "@types/node": "22.19.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "@vue/test-utils": "2.4.6",
    "bumpp": "10.3.1",
    "eslint": "9.39.1",
    "eslint-plugin-nuxt": "4.0.0",
    "happy-dom": "17.6.3",
    "husky": "9.1.7",
    "lint-staged": "16.2.6",
    "npm-run-all2": "8.0.4",
    "nuxt": "4.2.0",
    "sass": "1.93.3",
    "tailwindcss": "3.4.18",
    "typescript": "5.9.3",
    "vitest": "3.2.4",
    "vue-tsc": "3.1.3",
    "zlib-sync": "0.1.10"
  },
  "engines": {
    "node": ">=22 <23",
    "pnpm": ">=10 <11"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "zlib-sync"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue,md,json,yml}": "eslint"
  }
