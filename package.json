  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@fontsource/roboto": "5.2.6",
    "@nuxt/devtools": "2.5.0",
    "@nuxt/test-utils": "3.19.1",
    "@nuxtjs/i18n": "9.5.6",
    "@nuxtjs/tailwindcss": "6.14.0",
    "@rotki/eslint-config": "4.1.0",
    "@rotki/ui-library": "2.4.3",
